# 数据库配置
DATABASE_URL=mysql+pymysql://YOURUSER:YOURPASSWORD@MYSQLIP:PORT/handwriting_recognition?charset=utf8mb4
# 请配置 YOURUSER YOURPASSWORD MYSQLIP PORT
# 如果数据库名有必要更改，请更改

# JWT配置
SECRET_KEY=
# 请使用随机的SECRET_KEY，以保证安全性。
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# 推理服务配置
INFERENCE_SERVICE_HOST=localhost
INFERENCE_SERVICE_PORT=50051

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# 文件存储配置
UPLOAD_DIR=./uploads
SAMPLES_DIR=./uploads/samples
MODELS_DIR=./models

# 文件上传配置
MAX_UPLOAD_SIZE=10485760
# 单位：字节，默认10MB (10 * 1024 * 1024 = 10485760)

# CORS配置
CORS_ORIGINS=http://localhost:3000,http://localhost:5173
